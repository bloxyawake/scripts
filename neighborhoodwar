--go ahead skids i dont care i scripted this when was bored

local JointsService = game:GetService("JointsService")

local remoteEvent = JointsService:FindFirstChildOfClass("RemoteEvent")

if remoteEvent then
    local remoteFunction = remoteEvent:FindFirstChild("")

    if remoteFunction and remoteFunction:IsA("RemoteFunction") then
        remoteFunction.Parent = JointsService
        remoteEvent:Destroy()
    end
end

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "NW hak no way",
   Icon = 0,
   LoadingTitle = "free script omagad",
   LoadingSubtitle = "by haker",
   ShowText = "NW",
   Theme = "Default",
   ToggleUIKeybind = "K",
   DisableRayfieldPrompts = true,
   DisableBuildWarnings = false,
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "NeighborhoodWar",
      FileName = "Admin1337Pro"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },
   KeySystem = false
})

local MTab = Window:CreateTab("Modify Guns", 4483362458)

local is_exploit_function = is_synapse_function or iskrnlclosure or isourclosure or isexecutorclosure or is_sirhurt_closure or issentinelclosure or is_protosmasher_closure or function(...) return "is_exploit_function was not found in exploit environment" end

local norecoil = MTab:CreateButton({
   Name = "no recoil",
   Callback = function()
       for i,v in pairs(getgc()) do
           if type(v) == "function" and islclosure(v) and not is_exploit_function(v) and tostring(getfenv(v).script) == "client_main" then
               local lol = getconstants(v)
               if table.find(lol, "math") and table.find(lol, "random") and table.find(lol, "rad") then
                   for i2,v2 in pairs(getupvalues(v)) do
                       if type(v2) == "number" then
                           setupvalue(v,i2,0)
                       end
                   end
               end
           end
       end
   end,
})

local nospread = MTab:CreateButton({
   Name = "no spread",
   Callback = function()
       for i,v in pairs(getgc()) do
           if type(v) == "function" and islclosure(v) and not is_exploit_function(v) and tostring(getfenv(v).script) == "client_main" then
               local lol = getconstants(v)
               if table.find(lol, "math") and table.find(lol, "random") and table.find(lol, "PumpAction") and table.find(lol, "Vector3") then
                   setconstant(v, 15, "cos")
               end
           end
       end
   end,
})

local maxdistance = MTab:CreateButton({
   Name = "max distance",
   Callback = function()
       for i,v in pairs(getgc(true)) do
           if typeof(v) == "table" and rawget(v, "maxdistance") then
               rawset(v, "maxdistance", 9999)
           end
       end
   end,
})

local PTab = Window:CreateTab("Player", 4483362458)

local walkspeed = PTab:CreateSlider({
   Name = "Walkspeed",
   Range = {0, 60},
   Increment = 1,
   Suffix = "Walkspeed",
   CurrentValue = 16,
   Flag = "Walkspeed",
   Callback = function(Ws)
       local player = game.Players.LocalPlayer
       if player.Character then
           local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
           if humanoid then
               humanoid.WalkSpeed = Ws
           end
       end
   end,
})

local jumppower = PTab:CreateSlider({
   Name = "Jumppower",
   Range = {0, 250},
   Increment = 10,
   Suffix = "Jumppower",
   CurrentValue = 50,
   Flag = "jumppower",
   Callback = function(JP)
       local player = game.Players.LocalPlayer
       if player.Character then
           local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
           if humanoid then
               humanoid.JumpPower = JP
           end
       end
   end,
})

local VTab = Window:CreateTab("Visual", 4483362458)

local espConnections = {}
local espHighlights = {}

local esp = VTab:CreateToggle({
   Name = "ESP Enabled",
   CurrentValue = false,
   Flag = "ESP",
   Callback = function(ESP)
       if ESP then
           local function addESP(character, player)
               if player and player ~= game.Players.LocalPlayer then
                   local torso = character:FindFirstChild("Torso") or character:FindFirstChild("UpperTorso")
                   if torso then
                       local highlight = Instance.new("Highlight")
                       highlight.FillTransparency = 1
                       highlight.OutlineTransparency = 0
                       highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                       highlight.OutlineColor = torso.Color
                       highlight.Parent = character
                       highlight.Adornee = character
                       table.insert(espHighlights, highlight)
                   end
               end
           end
           
           for _, player in pairs(game.Players:GetPlayers()) do
               if player ~= game.Players.LocalPlayer and player.Character then
                   addESP(player.Character, player)
               end
           end
           
           local function onCharacterAdded(character)
               task.wait(2)
               local player = game.Players:GetPlayerFromCharacter(character)
               addESP(character, player)
           end
           
           for _, player in pairs(game.Players:GetPlayers()) do
               if player ~= game.Players.LocalPlayer then
                   local connection = player.CharacterAdded:Connect(onCharacterAdded)
                   table.insert(espConnections, connection)
               end
           end
           
           local playerAddedConnection = game.Players.PlayerAdded:Connect(function(player)
               local connection = player.CharacterAdded:Connect(onCharacterAdded)
               table.insert(espConnections, connection)
           end)
           table.insert(espConnections, playerAddedConnection)
           
       else
           for _, highlight in pairs(espHighlights) do
               if highlight and highlight.Parent then
                   highlight:Destroy()
               end
           end
           espHighlights = {}
           
           for _, connection in pairs(espConnections) do
               if connection then
                   connection:Disconnect()
               end
           end
           espConnections = {}
       end
   end,
})

local holeBeastConnections = {}
local holeBeastHighlights = {}

local espHoleBeast = VTab:CreateToggle({
   Name = "ESP Hole Beast",
   CurrentValue = false,
   Flag = "ESPHoleBeast",
   Callback = function(HESP)
       if HESP then
           local function addHoleBeastESP(holeBeast)
               local highlight = Instance.new("Highlight")
               highlight.FillTransparency = 1
               highlight.OutlineTransparency = 0
               highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
               highlight.OutlineColor = Color3.new(1, 0, 0)
               highlight.Parent = holeBeast
               highlight.Adornee = holeBeast
               table.insert(holeBeastHighlights, highlight)
           end
           
           for _, holeBeast in pairs(workspace:GetChildren()) do
               if holeBeast.Name == "hole beast" then
                   addHoleBeastESP(holeBeast)
               end
           end
           
           local connection = workspace.ChildAdded:Connect(function(child)
               if child.Name == "hole beast" then
                   addHoleBeastESP(child)
               end
           end)
           table.insert(holeBeastConnections, connection)
           
       else
           for _, highlight in pairs(holeBeastHighlights) do
               if highlight and highlight.Parent then
                   highlight:Destroy()
               end
           end
           holeBeastHighlights = {}
           
           for _, connection in pairs(holeBeastConnections) do
               if connection then
                   connection:Disconnect()
               end
           end
           holeBeastConnections = {}
       end
   end,
})

